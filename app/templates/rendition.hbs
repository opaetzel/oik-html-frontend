<!-- BEGIN PAGE CONTENT -->

<div class="container" id="main-container">
    <table class="table table-responsive">
        <thead>
            <tr>
                <td></td>
                {{#each model.maxWitnessArr as |num index|}}
                    <td>Zeuge {{num}}</td>
                {{/each}}
                <td>Gesamteindruck</td>
            </tr>
        </thead>
        <tbody>
            {{#each model.results as |pageResult index|}}
                <tr>
                    <td>Zeugenbefragung {{add index 1}}</td>
                    {{#each model.maxWitnessArr as |num index|}}
                        {{#if (getObjAt pageResult.rowResults index)}}
                            <td>{{getObjAt pageResult.rowResults index 'decision'}}</td>
                        {{else}}
                            <td>{{index}}-{{num}}</td>
                        {{/if}}
                    {{/each}}
                    <td>{{pageResult.totalResult}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
        
